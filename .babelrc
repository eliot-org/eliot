{
  "comments": false,
  "env": {
    "main": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "targets": {
              "node": 8
            }
          }
        ]
      ],
      "sourceType": "unambiguous"
    },
    "renderer": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false
          }
        ]
      ],
      "sourceType": "unambiguous"
    },
    "web": {
      "presets": [
        [
          "@babel/preset-env",
          {
            "modules": false
          }
        ]
      ],
      "sourceType": "unambiguous"
    }
  },
  "plugins": [
    "@babel/plugin-transform-runtime",
    "@babel/plugin-proposal-function-bind"
  ]
}
